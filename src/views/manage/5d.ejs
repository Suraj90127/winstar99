  <script>
    function changeDataAttribute(element, url) {
    document.documentElement.setAttribute("data-change", url); // Update attribute

    // Remove existing "Active" label from all items
    document.querySelectorAll(".col-12.col-sm-6.col-md-3 .active-label").forEach(label => label.remove());

    // Create the "Active" label
    const activeLabel = document.createElement("p");
    activeLabel.className = "active-label absolute top-0 right-0 bg-blue-800 text-white rounded-full px-2";
    activeLabel.style = "position: absolute; z-index: 99; right: 10px; background-color: #dc3545; padding: 0px 6px; border-radius: 5%; top:0;";
    activeLabel.textContent = "Active";

    // Append the label to the clicked item
    element.appendChild(activeLabel);
  }
  
    window.onload = function () {
    let firstItem = document.querySelector(".game-box");
    if (firstItem) {
      changeDataAttribute(firstItem, 1);
    }
  };
  
    </script>
  <script>
  
  
    $(".start-order").click(function (e) { 
      e.preventDefault();
      let game = $('html').attr('data-change');
      let value = $('#editResult').val().trim();
      let arr = value.split('|');
      for (let i = 0; i < arr.length; i++) {
          let check = isNumber(arr[i]);
          if (arr[i] == "" || arr[i].length != 5 || !check) {
              alert("Vui lòng nhập đúng định dạng (VD: 53278|67368|46826)");
              return false;
          }
      }
      $.ajax({
        type: "POST",
        url: "/api/webapi/admin/5d/editResult",
        data: {
          game: game,
          list: value,
        },
        dataType: "json",
        success: function (response) {
          Swal.fire(
              'Good job!',
              `${response.message}`,
              'success'
          );
          $('#ketQua').text(`next result: ${value}`);
        }
      });
    });
  </script>
